{
  "private": true,
  "scripts": {
    "dev": "mix",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "preprod": "npm run skulpt",
    "prod": "mix --production",
    "preproduction": "npm run skulpt",
    "production": "mix --production",
    "skulpt": "npm --prefix node_modules/skulpt install --legacy-peer-deps && npm --prefix node_modules/skulpt run build",
    "refreshskulpt": "npm uninstall skulpt && npm install pingskills/skulpt#pyangelo --save-dev"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "bootstrap-sass": "^3.4.1",
    "canvas-confetti": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.17.0",
    "eslint-webpack-plugin": "^3.0.1",
    "font-awesome": "^4.7.0",
    "howler": "^2.2.3",
    "jest": "^28.1.3",
    "jquery": "^3.5.1",
    "laravel-mix": "^6.0.11",
    "laravel-mix-eslint": "^0.2.0",
    "postcss": "^8.2.6",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.39.2",
    "sass-loader": "^12.1.0",
    "skulpt": "github:pingskills/skulpt#pyangelo",
    "split-grid": "^1.0.11",
