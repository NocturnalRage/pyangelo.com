{
  "private": true,
  "scripts": {
    "dev": "mix",
    "development": "mix",
    "testjs": "node_modules/.bin/jest",
    "test": "node_modules/.bin/jest && vendor/bin/phpunit",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "preprod": "npm run skulpt",
    "prod": "mix --production",
    "preproduction": "npm run skulpt",
    "production": "mix --production",
    "skulpt": "npm --prefix node_modules/skulpt install --legacy-peer-deps && npm --prefix node_modules/skulpt run build",
    "refreshskulpt": "npm uninstall skulpt && npm install pingskills/skulpt#pyangelo --save-dev"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "bootstrap-sass": "^3.4.1",
    "canvas-confetti": "^1.4.0",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-webpack-plugin": "^4.2.0",
    "font-awesome": "^4.7.0",
    "howler": "^2.2.3",
    "jest": "^29.7.0",
    "jquery": "^3.5.1",
    "laravel-mix": "^6.0.11",
    "laravel-mix-eslint": "^0.2.0",
    "postcss": "^8.2.6",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.39.2",
    "sass-loader": "^16.0.1",
    "skulpt": "github:pingskills/skulpt#pyangelo",
    "split-grid": "^1.0.11",
    "tinymce": "^7.0.1",
    "toastify-js": "^1.11.2"
  }
}
